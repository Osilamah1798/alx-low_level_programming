#include <stdio.h>
#include "main.h"
/**
 * _atoi - converts a string to an integer
 * main - write a program should print the result of the multiplication
 * @argc: number of arguments
 * @argv: array of arguments
 * followed by a new line
 *
 * Return: Always 0 (Success)
 */
int main(int argc, char *argv[])
{
int i, sum = 0;

if(argc > 1)
{
for (i = 1; i < argc; i++)
{
printf("%d\n", sum);

 sum *= _atoi(argv[i]);
}
}
return (0);
}
